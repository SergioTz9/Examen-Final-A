@model ControlCatedratico.Models.CursosModel
@{
    ViewData["Title"] = "Producto";
    Layout = "~/Views/Shared/_Layout.cshtml";

}



<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0 text-dark">Consultar de Curso</h1>
                </div><!-- /.col -->
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Pureba</li>
                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>

    <hr />
    <div class="col-md-12">
        <dl class="row">

            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.CodigoCurso)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CodigoCurso)
            </dd>

            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.NombreCurso)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.NombreCurso)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.FechaInicio)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.FechaInicio)
            </dd>

            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.FechaFinaliza)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.FechaFinaliza)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.CodigoCatedratico)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CodigoCatedratico)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.EstadoCurso)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.EstadoCurso)
            </dd>

        </dl>
    </div>
    <a asp-action="InicioCurso">Regresar</a>
</div>
